<table class="table">
  <h1>User info</h1>
  <tbody>
  <%= image_tag user.get_profile_image(100,100) %>
  <tr>
    <th>name</th>
    <th><%= user.name %></th>
  </tr>
  <tr>
    <th>introduction<th>
    <th><%= user.introduction %></th>
  </tr>
  </tbody>
</table>
 <div class="row">
   <%= link_to edit_user_path(user), class: "btn btn-info btn-sm btn-block mb-3" do %>
   <i class="fas fa-user-cog"></i>
   <% end %>
 </div>